 RowBox[{
  RowBox[{"diceEM", "[", 
   RowBox[{"trials_", ",", "maxIterations_", ",", "accuracy_"}], "]"}], 
  ":="}]], "Input",
 CellChangeTimes->{{3.5855894336454363`*^9, 3.585589433646515*^9}, {
